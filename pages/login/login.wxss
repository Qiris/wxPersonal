/* pages/login/login.wxss */
.container {
  height: 100vh;
  background-repeat: no-repeat;
  background-image: url("data:image/jpg;base64,")
}
.headerPic {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin-top: -15%;
}
.loginPanel {
  width: 500rpx;
  height: 400rpx;
  margin: 0 auto;
  position: relative;
  top:-250rpx;
  display: flex;
  flex-direction: column;
}
.loginPanel .formItem {
  flex: 1;
  margin: 20rpx 0;
  display: flex;
}
.account , .password {
  border-bottom: 1px solid lightcyan;
  flex: 1;
  margin-left: 20rpx;
  padding-left: 20rpx;
}
.container .iconfont {
  color: white;
}
.spaceBetween {
  justify-content: space-between;
}
.loginBtn , .registBtn {
  width: 100%;
  height: 80rpx;
  line-height: 150%;
  border-radius: 40rpx;
}